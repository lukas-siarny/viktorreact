{
  "name": "error_503",
  "version": "0.1.0",
  "author": "Daniel Hlavaty <daniel.hlavaty@goodrequest.com>",
  "homepage": "503/",
  "private": true,
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true npm-run-all -p start-watch translate-watch build:tailwind",
    "start-watch": "cross-env REACT_APP_VERSION=$npm_package_version react-scripts start",
    "prebuild": "cross-env NODE_ENV=production npm-run-all -p build:tailwind rebuild:sass",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true NODE_ENV=production REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "translate-scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "translate-watch": "npm run translate-scan && gulp watch",
    "rebuild:sass": "npm rebuild node-sass",
    "export-tailwind-config": "tailwindcss-export-config --config=tailwind.config.js --destination=src/styles/tailwind-variables --flat=true --format=sass --quoted-keys=true",
    "build:tailwind": "npm run export-tailwind-config && tailwind build src/styles/tailwind.css -o src/styles/main.css",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "dependencies": {
    "antd": "file:../node_modules/antd",
    "autoprefixer": "file:../node_modules/autoprefixer",
    "classnames": "file:../node_modules/classnames",
    "customize-cra": "file:../node_modules/customize-cra",
    "gulp": "file:../node_modules/gulp",
    "i18next": "file:../node_modules/i18next",
    "i18next-browser-languagedetector": "file:../node_modules/i18next-browser-languagedetector",
    "i18next-scanner": "file:../node_modules/i18next-scanner",
    "npm-run-all": "file:../node_modules/npm-run-all",
    "postcss": "file:../node_modules/postcss",
    "qs": "file:../node_modules/qs",
    "react": "file:../node_modules/react",
    "react-dom": "file:../node_modules/react-dom",
    "react-i18next": "file:../node_modules/react-i18next",
    "react-router-dom": "file:../node_modules/react-router-dom",
    "react-scripts": "file:../node_modules/react-scripts",
    "sass": "file:../node_modules/sass",
    "tailwindcss": "file:../node_modules/tailwindcss",
    "tailwindcss-export-config": "file:../node_modules/tailwindcss-export-config"
  },
  "devDependencies": {
    "@types/classnames": "file:../node_modules/@types/classnames",
    "@types/qs": "file:../node_modules/@types/qs",
    "@types/react": "file:../node_modules/@types/react",
    "@types/react-dom": "file:../node_modules/@types/react-dom",
    "@types/react-i18next": "file:../node_modules/@types/react-i18next",
    "cross-env": "file:../node_modules/cross-env",
    "postcss-loader": "file:../node_modules/postcss-loader",
    "typescript": "file:../node_modules/typescript"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
