{
  "name": "notino_b2b_web",
  "version": "2.1.0-rc.10",
  "main": "src/index.ts",
  "repository": "git@bitbucket.org:GoodRequest/notino_b2b_web.git",
  "author": "Jan Cvencek <jan.cvencek@goodrequest.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "cross-env PROXY=https://notino-admin.goodrequest.dev npm-run-all -p generate-runtime-env start-watch translate-watch build:tailwind",
    "start-watch": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true REACT_APP_VERSION=$npm_package_version react-scripts start",
    "prebuild": "cross-env NODE_ENV=production SKIP_PREFLIGHT_CHECK=true npm-run-all -p build:tailwind rebuild:sass",
    "copy-modules": "node scripts/postBuild.js",
    "copy-build": "node scripts/copyErrorBuild.js",
    "postbuild": "cross-env SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true npm run copy-modules && npm --prefix ./error_503 run build && npm run copy-build",
    "build": "cross-env NODE_ENV=production SKIP_PREFLIGHT_CHECK=true REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build:coverage": "cross-env CYPRESS_INSTRUMENT_PRODUCTION=true NODE_ENV=production SKIP_PREFLIGHT_CHECK=true REACT_APP_VERSION=$npm_package_version react-scripts -r @cypress/instrument-cra build",
    "lint": "eslint src --color --ext .js --ext .ts --ext .jsx --ext .tsx",
    "release": "npm run build && npm run sentry",
    "sentry": "cross-env REACT_APP_VERSION=$npm_package_version node sentry.js",
    "test": "cross-env AUTH_EMAIL=super.admin@goodrequest.com AUTH_PASSWORD=Lopaty123. SIGN_IN_URL=http://localhost:3000/api/b2b/admin/auth/login cypress open",
    "test:CI": "cross-env AUTH_EMAIL=super.admin@goodrequest.com AUTH_PASSWORD=Lopaty123. SIGN_IN_URL=http://localhost:3000/api/b2b/admin/auth/login cypress run",
    "generate-coverage-summary": "npx nyc report --reporter=text-summary",
    "eject": "react-scripts eject",
    "translate-scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "translate-watch": "npm run translate-scan && gulp watch",
    "rebuild:sass": "npm rebuild node-sass",
    "export-tailwind-config": "tailwindcss-export-config --config=tailwind.config.js --destination=src/styles/tailwind-variables --flat=true --format=sass --quoted-keys=true",
    "build:tailwind": "npm run export-tailwind-config && tailwind build src/styles/tailwind.css -o src/styles/main.css",
    "type-check": "tsc --noEmit",
    "normalize-apidoc": "node scripts/normalizeApidoc.js",
    "typegen": "typegen apidoc/apidoc-normalize.json > src/types/api.d.ts",
    "download:apidoc": "cross-env APIDOC=https://notino-admin.goodrequest.dev/api/doc/data.json node scripts/downloadApidoc.js",
    "generate-ts-api": "npm run download:apidoc && npm run normalize-apidoc && npm run typegen",
    "translations-csv-to-json": "npx convert-csv-to-json GoodRequest/i18nextJsonToCsv",
    "translations-json-to-csv": "npx convert-json-to-csv GoodRequest/i18nextJsonToCsv",
    "wait-for-it": "./scripts/wait-for-it.sh",
    "generate-runtime-env": "cross-env env-cmd runtime-env-cra --config-name=./public/runtime-env.js --env-file=.env.template"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.5.6",
