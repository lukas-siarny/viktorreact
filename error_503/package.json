{
  "name": "project_template_tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p start-watch translate-watch build:tailwind",
    "start-watch": "cross-env REACT_APP_VERSION=$npm_package_version react-app-rewired start",
    "prebuild": "cross-env NODE_ENV=production npm-run-all -p build:tailwind rebuild:sass",
    "build": "cross-env NODE_ENV=production REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "translate-scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "translate-watch": "npm run translate-scan && gulp watch",
    "rebuild:sass": "npm rebuild node-sass",
    "export-tailwind-config": "tailwindcss-export-config --config=tailwind.config.js --destination=src/styles/tailwind-variables --flat=true --format=sass --quoted-keys=true",
    "build:tailwind": "npm run export-tailwind-config && tailwind build src/styles/tailwind.css -o src/styles/main.css",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "dependencies": {
    "antd": "4.22.5",
    "autoprefixer": "10.4.8",
    "babel-plugin-import": "1.13.5",
    "classnames": "2.3.1",
    "customize-cra": "1.0.0",
    "customize-cra-less-loader": "2.0.0",
    "gulp": "4.0.2",
    "i18next": "21.9.0",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-chained-backend": "3.0.2",
    "i18next-http-backend": "1.4.1",
    "i18next-localstorage-backend": "3.1.3",
    "i18next-scanner": "4.0.0",
    "less": "4.1.3",
    "less-loader": "11.0.0",
    "node-sass": "7.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.18.3",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "3.1.8",
    "tailwindcss-export-config": "4.1.0"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-i18next": "8.1.0",
    "cross-env": "7.0.3",
    "postcss-loader": "7.0.1",
    "react-app-rewired": "2.2.1",
    "typescript": "4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
