{
  "name": "notino_b2b_web",
  "version": "0.0.10-demo.1",
  "main": "src/index.ts",
  "repository": "git@bitbucket.org:GoodRequest/notino_b2b_web.git",
  "author": "Jan Cvencek <jan.cvencek@goodrequest.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16.13.0"
  },
  "scripts": {
    "start": "cross-env PORT=3210 npm-run-all -p start-watch translate-watch build:tailwind",
    "start-watch": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true REACT_APP_VERSION=$npm_package_version react-app-rewired start",
    "prebuild": "cross-env NODE_ENV=production SKIP_PREFLIGHT_CHECK=true  npm-run-all -p build:tailwind rebuild:sass",
    "build": "cross-env NODE_ENV=production SKIP_PREFLIGHT_CHECK=true REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "lint": "eslint src --color --ext .js --ext .ts --ext .jsx --ext .tsx",
    "release": "npm run build && npm run sentry",
    "sentry": "cross-env REACT_APP_VERSION=$npm_package_version node sentry.js",
    "test": "cypress open",
    "eject": "react-scripts eject",
    "translate-scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "translate-watch": "npm run translate-scan && gulp watch",
    "rebuild:sass": "npm rebuild node-sass",
    "export-tailwind-config": "tailwindcss-export-config --config=tailwind.config.js --destination=src/styles/tailwind-variables --flat=true --format=sass --quoted-keys=true",
    "build:tailwind": "npm run export-tailwind-config && tailwind build src/styles/tailwind.css -o src/styles/main.css",
    "type-check": "tsc --noEmit",
    "normalize-apidoc": "node scripts/normalizeApidoc.js",
    "typegen": "typegen apidoc/apidoc-normalize.json > src/types/api.d.ts",
    "download:apidoc": "cross-env APIDOC=https://notino-development.goodrequest.dev/api/doc/data.json node scripts/downloadApidoc.js",
    "generate-ts-api": "npm run download:apidoc && npm run normalize-apidoc && npm run typegen",
    "translations-csv-to-json": "npx convert-csv-to-json GoodRequest/i18nextJsonToCsv",
    "translations-json-to-csv": "npx convert-json-to-csv GoodRequest/i18nextJsonToCsv",
    "storybook": "npm run build:tailwind && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@sentry/cli": "1.71.0",
    "@sentry/react": "6.19.6",
    "@sentry/tracing": "6.19.6",
    "@soywod/pin-field": "0.2.2",
    "antd": "4.18.2",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "autoprefixer": "10.4.4",
    "axios": "0.27.2",
    "babel-plugin-import": "1.13.5",
    "classnames": "2.3.1",
    "core-js": "3.22.0",
    "cross-env": "7.0.3",
    "customize-cra": "1.0.0",
    "dayjs": "1.11.1",
    "eslint-plugin-prettier": "4.0.0",
    "flagpack-core": "2.0.0",
    "gulp": "4.0.2",
    "history": "4.10.1",
    "http-proxy-middleware": "2.0.4",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-chained-backend": "3.0.2",
    "i18next-localstorage-backend": "3.1.3",
    "i18next-scanner": "3.1.0",
    "i18next-xhr-backend": "3.2.2",
    "jwt-decode": "3.1.2",
    "less": "4.1.2",
    "less-loader": "7.3.0",
    "lodash": "4.17.21",
    "lodash-checkit": "2.4.1",
    "npm-run-all": "4.1.5",
    "openapi-client-axios": "5.1.2",
    "openapi-client-axios-typegen": "5.0.2",
    "postcss": "8.4.12",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-app-rewire-hot-loader": "2.0.1",
    "react-app-rewired": "2.2.1",
    "react-dom": "17.0.2",
    "react-easy-crop": "4.1.4",
    "react-geocode": "0.2.3",
    "react-google-maps": "9.4.5",
    "react-i18next": "11.16.7",
    "react-idle-timer": "4.6.4",
    "react-input-mask": "2.0.4",
    "react-pin-field": "2.0.0-beta.3",
    "react-redux": "8.0.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-router-navigation-prompt": "1.9.6",
    "react-scripts": "4.0.3",
    "redux": "4.2.0",
    "redux-form": "8.3.8",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.1",
    "regenerator-runtime": "0.13.9",
    "sass": "1.50.1",
    "slugify": "1.6.5",
    "tailwindcss": "2.2.19",
    "tailwindcss-export-config": "2.3.0",
    "use-query-params": "1.2.3"
  },
  "devDependencies": {
    "@goodrequest/eslint-config-typescript-react": "1.1.1",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
